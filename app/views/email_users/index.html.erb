<p id="notice"><%= notice %></p>

<h1>Listing Email Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Login</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @email_users.each do |email_user| %>
      <tr>
        <td><%= email_user.name %></td>
        <td><%= email_user.email %></td>
        <td><%= email_user.login %></td>
        <td><%= link_to 'Show', email_user %></td>
        <td><%= link_to 'Edit', edit_email_user_path(email_user) %></td>
        <td><%= link_to 'Destroy', email_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Email user', new_email_user_path %>
